{
  "name": "react-flexible-workbench",
  "version": "5.4.0",
  "description": "Provides a flexible workspace UI for single-page web apps based on GoldenLayout",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run build:lib",
    "build:lib": "tsc && copyfiles -u 1 src/*.css lib",
    "dist": "npm run build && npm run dist:file && npm run dist:minify",
    "dist:file": "webpack --config webpack.build.js",
    "dist:minify": "MINIFY=1 npm run dist:file",
    "clean": "rimraf lib dist",
    "lint": "tslint --project tsconfig.json --type-check --format stylish",
    "preversion": "npm test",
    "start": "webpack serve --config webpack.config.js --content-base examples/",
    "version": "npm run build",
    "postversion": "git push && git push --tags",
    "test": "npm run build"
  },
  "author": "Tam√°s Nepusz <ntamas@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ntamas/react-flexible-workbench.git"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.5",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^14.14.36",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^17.0.3",
    "@types/react-beautiful-dnd": "^13.0.0",
    "@types/react-dom": "^17.0.3",
    "copyfiles": "^2.4.1",
    "css-loader": "^5.2.0",
    "delay": "^5.0.0",
    "jquery": "3.6.0",
    "react": "^17",
    "react-dom": "^17",
    "react-shapeshifter": "^1.0.9",
    "react-spinners": "^0.10.6",
    "rimraf": "^3.0.2",
    "source-map-loader": "^2.0.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.18",
    "tslint": "^6.1.3",
    "typescript": "^4.2.3",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "arrive": "^2.4.1",
    "eventemitter3": "^4.0.7",
    "golden-layout": "^1.5.9",
    "javascript-detect-element-resize": "^0.5.3",
    "lodash-es": "^4.17.21",
    "mutation-observer": "^1.0.3",
    "prop-types": "^15.7.2",
    "react-badger": "^2.0.6",
    "react-beautiful-dnd": "^13.1.0"
  },
  "peerDependencies": {
    "jquery": "^3.6.0",
    "react": "^16.3.2 || ^17",
    "react-dom": "^16.3.2 || ^17"
  }
}
